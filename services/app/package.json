{
    "name": "accntu",
    "version": "0.1.0",
    "private": true,
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "db:generate": "drizzle-kit generate:pg --config=drizzle/config.ts",
        "db:migrate": "tsx ./drizzle/migrate.ts",
        "db:studio": "drizzle-kit studio --config=drizzle/config.ts",
        "db:introspect": "drizzle-kit introspect:pg --config=drizzle/config.ts",
        "db:seed": "tsx ./drizzle/seed.ts",
        "docker:dev": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml --project-directory . up -d --build --force-recreate ",
        "docker:db": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml --project-directory . up db -d --build --force-recreate ",
        "docker:test": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.prod.yml -f docker/docker-compose.test.yml --project-directory . up -d --build --force-recreate"
    },
